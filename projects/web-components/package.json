{
    "name": "datapane-next",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "build:alpine-stores": "node_modules/.bin/vite build --config alpine-stores/vite.config.ts",
        "build:base": "node_modules/.bin/vite build --config base/vite.config.ts",
        "build:report": "node --max_old_space_size=16384 node_modules/.bin/vite build --config report/vite.config.ts",
        "build:params": "node --max_old_space_size=16384 node_modules/.bin/vite build --config params-component/vite.config.ts",
        "build:local-report": "node --max_old_space_size=16384 node_modules/.bin/vite build --config report/local-report.vite.config.ts",
        "build": "npm run build:base && npm run build:report && npm run build:local-report && npm run build:params && npm run build:alpine-stores",
        "dev:report": "vite build --mode development --config base/vite.config.ts && vite build --watch --mode development --config report/vite.config.ts && npm run dev:tailwind",
        "dev:params": "vite build --mode development --config base/vite.config.ts && vite build --watch --mode development --config params-component/vite.config.ts",
        "test:public": "node_modules/.bin/cypress run --spec \"cypress/e2e/public/*\"",
        "test:org": "node_modules/.bin/cypress run --spec \"cypress/e2e/org/*\"",
        "test:open": "node_modules/.bin/cypress open --browser electron",
        "typecheck": "node_modules/.bin/vue-tsc --noEmit",
        "storybook": "start-storybook -p 6006",
        "build:storybook": "node --max_old_space_size=16384 node_modules/.bin/build-storybook -o dist/storybook"
    },
