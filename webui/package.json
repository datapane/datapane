{
    "name": "datapane-next",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "build:base": "node_modules/.bin/vite build --config base/vite.config.ts",
        "build:report": "node --max_old_space_size=16384 node_modules/.bin/vite build --config report/vite.config.ts",
        "build:local-report": "node --max_old_space_size=16384 node_modules/.bin/vite build --config report/local-report.vite.config.ts",
        "build": "npm run build:base && npm run build:report && npm run build:local-report",
        "dev:report": "vite build --mode development --config base/vite.config.ts && vite build --watch --mode development --config report/vite.config.ts",
        "test:public": "node_modules/.bin/cypress run --spec \"cypress/e2e/public/*\"",
        "test:org": "node_modules/.bin/cypress run --spec \"cypress/e2e/org/*\"",
        "test:open": "node_modules/.bin/cypress open --browser electron",
        "typecheck": "node_modules/.bin/vue-tsc --noEmit",
        "storybook": "start-storybook -p 6006"
    },
    "dependencies": {
        "@bokeh/bokehjs": "~2.4.2",
